## Process this file with automake to produce Makefile.in
## src/GrLib/Bitmaps directory makefile.

AUTOMAKE_OPTIONS = foreign

bmpFiles	= arrow.bmp tool1.bmp tool2.bmp tool3.bmp tool4.bmp \
				new.bmp open.bmp save.bmp cut.bmp print.bmp help.bmp play.bmp \
				stop.bmp

xpmFiles	= dsam.xpm arrow.xpm tool1.xpm tool2.xpm tool3.xpm tool4.xpm \
				new.xpm open.xpm save.xpm cut.xpm print.xpm help.xpm play.xpm \
				stop.xpm

EXTRA_DIST	= $(bmpFiles) $(xpmFiles)
